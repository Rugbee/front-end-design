}if(isGroupInstance){var detailsFieldset=Y$("fieldset.user-share-details",form,true),detailsTextarea=Y$("textarea",detailsFieldset,true),detailsLink=Y$("a.add-details",form,true),subjectLine=Y$("textarea.post-message",form,true),subjectLabel=Y$("label.ghost.subject",form,true),detailsLabel=Y$("label.ghost.details",form,true),originalUrl="",linkornews=Y$("#attach a",detailsFieldset,true),submitTop=Y$("#share-submit.top",form,true),submitBottom=Y$("#share-submit.bottom",form,true),urlFromSubjectLine=false
}var getLinkObjectRequest;
if(config.focusOnPageLoad){el.focus();
