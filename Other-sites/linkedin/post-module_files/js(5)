}function a(d){YEvent.preventDefault(d);
LI.Dialog().open({name:"sharingDialog",type:"task-modeless",width:500,content:{title:b.title,url:b.url},dependencies:{jsFiles:jsFiles,cssFiles:cssFiles},showOnlyWhenReady:b.showOnlyWhenReady})
}YEvent.on(c,"click",a)
};

