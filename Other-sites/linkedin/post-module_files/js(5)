LI.Share=function(c,b){if(LI.__HPA===true){console.info("HOMPAGE_PERFORMANCE_ANALYSIS :: /js/apps/Share.js")
}b={url:(b.url)?b.url:null,title:(b.title)?b.title:LI.i18n.get("share-dialog-title"),lazyEvent:(b.lazyEvent)?b.lazyEvent:null,showOnlyWhenReady:b.showOnlyWhenReady||false},jsFiles=LI.SharingDialog.Scripts,cssFiles=LI.SharingDialog.Styles;
