var m=null,n=YDom.getChildrenBy(a,function(q){return(!YDom.hasClass(q,"extra")&&!YDom.hasClass(q,"empty-message"))
}),p=document.getElementById("recJobs-empty-message"),o=document.getElementById("recjobs-see-more");
if(g.length){m=g[0];
YDom.removeClass(m,"extra");
g=g.slice(1)
}else{if(!n.length&&p){YDom.addClass(d,"empty");
m=p
}}if(!g.length){if(o&&!c.hasMoreRecJobs){LI.hide(o)
}}if(m){YDom.setStyle(m,"opacity",0);
var l=new YAnim(m,{opacity:{to:1}},0.8);
l.animate()
}});
k.animate()
};
f()
};

