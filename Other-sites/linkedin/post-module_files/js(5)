}}enableSubmit();
YEvent.on(form,"submit",postStatus);
function setTimer(){if(timeout){window.clearTimeout(timeout)
}timeout=window.setTimeout(grepUrl,1200)
}function grepUrl(){var shareText=el.value.replace(/\n/g," ").split(" "),match;
for(var i=0;
shareText.length>i;
i++){match=shareText[i].match(urlRegEx);
if(match&&(!match.index||match.input.charAt(match.index-1)!=="@")){getLinkObject(shareText[i]);
window.clearTimeout(timeout);
break
}}}function getLinkObject(url){if(!url){return
}if(YDom.hasClass(shareMode,"is-view-mode")||YDom.hasClass(shareMode,"is-edit-mode")){return
}if(isGroupInstance){originalUrl=url
}url=config.getObjectURL+encodeURIComponent(url);
shareMode.className="share-mode is-loading-mode";
ajaxRequest=YAHOO.util.Connect.asyncRequest("GET",url,{success:success,failure:getLinkObjectError,timeout:12000});
WebTracking.trackUserAction(config.trackGetLinkStart);
function success(o){if(isGroupInstance&&urlFromSubjectLine){if(YAHOO.lang.trim(subjectLine.value)==originalUrl){subjectLine.value=""
}urlFromSubjectLine=false
}if(!typeIsXML(o)){getLinkObjectSuccess(o)
}else{if(o.responseXML.getElementsByTagName("ticketStatusUrl")[0]){var url=o.responseXML.getElementsByTagName("ticketStatusUrl")[0].firstChild.nodeValue;
poll(url,{success:getLinkObjectSuccess,failure:getLinkObjectError})
}else{getLinkObjectError()
}}}}function getLinkObjectSuccess(o){if(!o.responseText||(typeIsXML(o)&&o.responseXML.getElementsByTagName("responseInfo")[0]&&o.responseXML.getElementsByTagName("responseInfo")[0].firstChild.nodeValue=="FAILURE")||LI.isFullPage(o.responseText)){LI.show(attach);
getLinkObjectError();
return
}var target=YDom.get("share-view");
target.innerHTML=o.responseText;
LI.Controls.parseFragment(target);
