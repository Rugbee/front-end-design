var timeout,ajaxRequest,pollTimeout,shareMode=YDom.get("share-mode"),shareView=YDom.get("share-view"),shareAjax=YDom.get("share-ajax"),gallery,editMeta=YDom.get("share-edit-meta"),editSummary=YDom.get("share-edit-summary"),editTitle=YDom.get("share-edit-title"),entityID=YDom.get("share-entity-id"),entityURL=YDom.get("share-entity-url"),imageID=YDom.get("share-img-selected-url"),submit=YDom.get("share-submit"),settings=YDom.get("share-settings"),urlRegEx=(config.urlRegEx)?config.urlRegEx:LI.ShareUrlRegEx,viewMeta,viewSummary,viewTitle,attach=YDom.get("attach"),attachLinks=Y$("a","attach"),attachInput=YDom.get("share-attach-input"),attachSubmit=YDom.get("share-attach-link"),form=YDom.get("share-form"),isSlickShareBox=(!YAHOO.lang.isNull(YDom.get("slick-sharing-cont"))),isGroupInstance=(config.injectionMode=="group"),shareBoxAnimAttr={},shareBoxAnim=null,ANIMATION_DURATION=0.2;
