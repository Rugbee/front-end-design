f.src=n;
h.innerHTML=m+1;
if(YDom.get(e.imgInputID)){YDom.get(e.imgInputID).value=n
}if(YDom.get(e.imgSelectedIdxID)){YDom.get(e.imgSelectedIdxID).value=e.current
}}function c(){var p=[];
var o=1;
var m;
var r=document.createElement("div");
document.body.appendChild(r);
YDom.setStyle(r,"position","absolute");
YDom.setStyle(r,"left","70000px");
for(var n=0;
e.images.length>n;
n++){m=null;
if(n==0){p.push(e.images[0])
}else{m=document.createElement("img");
r.appendChild(m);
YEvent.on(m,"load",function(s){if(parseInt(YDom.getRegion(this).width)>60&&parseInt(YDom.getRegion(this).height)>60){p.push(this.src)
}o++;
q()
});
YEvent.on(m,"error",function(s){o++;
q()
});
m.src=e.images[n]
}}function q(){if(o==e.images.length){document.body.removeChild(r);
e.images=p;
if(p.length>1){LI.show(k)
}i.innerHTML=p.length;
YEvent.on([g,a],"click",l)
}}}function b(m){var n=YEvent.getTarget(m);
