}else{window.setTimeout(function(){LI.GhostLabel.Manager.show(form.id)
},0);
return
}}if(isGroupInstance&&urlFromSubjectLine){if(YAHOO.lang.trim(subjectLine.value)==originalUrl){subjectLine.value=""
}urlFromSubjectLine=false
}disableSubmit();
YAHOO.util.Connect.setForm(form);
YAHOO.util.Connect.asyncRequest("POST",form.action,{success:success,failure:failure,timeout:12000});
function success(o){if(typeIsXML(o)){if(o.responseXML.getElementsByTagName("formErrors")[0]){var errorObj=eval("("+o.responseXML.getElementsByTagName("formErrors")[0].firstChild.nodeValue+")");
LI.showFormErrors(o);
enableSubmit();
hideCheckTextareaMessage()
}else{if(o.responseXML.getElementsByTagName("responseInfo")[0]&&o.responseXML.getElementsByTagName("responseInfo")[0].firstChild.nodeValue=="DUPLICATE"){enableSubmit();
var msg=o.responseXML.getElementsByTagName("responseMsg")[0].firstChild.nodeValue;
LI.injectAlert(msg,"error");
el.value="";
resetHeight();
el.blur()
}else{if(o.responseXML.getElementsByTagName("responseInfo")[0]&&o.responseXML.getElementsByTagName("responseInfo")[0].firstChild.nodeValue=="FAILURE"){failure(o)
}else{if(o.responseXML.getElementsByTagName("jsonPayLoad")[0]){var payload=o.responseXML.getElementsByTagName("jsonPayLoad")[0].firstChild.nodeValue;
var obj=eval("("+payload+")");
if(obj.sharingUpdateUrl){poll(obj.sharingUpdateUrl)
}if(isGroupInstance&&!config.incompany){postStatusSucces(o,obj)
}}}}}}else{failure(o)
}}function failure(o){var msgNode,msg;
enableSubmit();
if(typeIsXML(o)){msgNode=o.responseXML.getElementsByTagName("responseMsg")[0];
if(msgNode){msg=msgNode.firstChild.nodeValue;
if(msg){LI.injectAlert(msg,"error")
}}}else{shareAjax.value="false";
LI.injectAlert(LI.i18n.get("error"),"error")
}}}function postStatusSucces(o,jsonPayloadObj){var postModuleURL,itemFeedURL;
function injectFeedItem(){if(config.injectionMode=="company"){itemFeedURL=config.itemFeedURL;
if(itemFeedURL){LI.asyncRequest("GET",itemFeedURL,{success:function(o){var activityEl=YDom.getElementsByClassName("activity","div")[0];
activityEl.innerHTML=o.responseText;
LI.Controls.parseFragment(activityEl)
}},null)
}}else{if(LI.NusInjection&&LI.NusInjection.injectFeedItem){LI.NusInjection.injectFeedItem(o.responseText)
}}}function replacePostModule(html){var postHome=YDom.getElementsByClassName("post-home","div")[0],newPostModule=LI.domify(html),cloneEl=newPostModule.cloneNode(false);
injectFeedItem();
postHome.parentNode.replaceChild(cloneEl,postHome);
cloneEl.innerHTML=newPostModule.innerHTML;
LI.Controls.parseFragment(cloneEl)
}function resetPostModule(){injectFeedItem();
removeObject();
resetHeight();
LI.hide(Y$(".post-home .my-current","body",true));
enableSubmit();
attachInput.value="";
LI.show(settings);
el.value="";
el.blur()
}if(config.injectionMode=="inject"||config.injectionMode=="company"){postModuleURL=config.postModuleURL;
if(postModuleURL){YAHOO.util.Connect.asyncRequest("GET",postModuleURL,{success:function(o){replacePostModule(o.responseText)
},failure:function(){resetPostModule()
}})
}else{resetPostModule()
}}else{if(isGroupInstance&&LI.AnetCarousel){LI.AnetCarousel.handlePost(jsonPayloadObj);
resetSharingForm()
}else{if(config.incompany){resetSharingForm();
injectFeedItem();
if(YDom.inDocument("feed-content")){var noResults=YDom.getElementsByClassName("no-results","div","feed-content");
if(noResults){LI.hide(noResults[0])
}}}else{if(isGroupInstance){if(jsonPayloadObj&&jsonPayloadObj.isPremoderated){LI.injectAlert(config.i18n.postSubmitted,"success");
resetSharingForm()
}else{location.reload(true)
}}else{if(config.injectionMode=="reload"){location.reload(true)
}else{if(config.injectionMode=="inject-only"){injectFeedItem();
enableSubmit();
LI.show(settings);
el.value="";
el.blur()
}}}}}}hideCheckTextareaMessage()
}function poll(url,cb){var callCount=1;
var callback={success:success,failure:failure,timeout:12000};
function makeRequest(){ajaxRequest=YAHOO.util.Connect.asyncRequest("GET",url,callback)
}makeRequest();
function success(o){if(!typeIsXML(o)){postStatusSucces(o)
}else{if(o.responseXML.getElementsByTagName("responseInfo")[0]){var status=o.responseXML.getElementsByTagName("responseInfo")[0].firstChild.nodeValue;
if(status=="TICKET_CLOSED"){url=o.responseXML.getElementsByTagName("forwardUrl")[0].firstChild.nodeValue;
callback=cb;
makeRequest()
}else{if(callCount<12){pollTimeout=window.setTimeout(makeRequest,1300);
callCount++
}else{failure(o)
}}}}}function failure(o){if(config.incompany){resetSharingForm()
}else{LI.grow(el,"15");
removeObject();
hideCheckTextareaMessage()
}}}function resetSharingForm(){removeObject();
attachInput.value="";
switch(config.injectionMode){case"group":enableSubmit();
detailsTextarea.value="";
YDom.removeClass(detailsFieldset,"open");
subjectLine.value="";
YDom.removeClass(form,"details");
YDom.removeClass(form,"attach");
break;
default:break
}}function setSubmitButtonRef(button){submit=(button)?button:submit
}function showEdit(){shareMode.className="share-mode is-edit-mode";
gallery=YDom.get("share-image");
YDom.insertBefore(gallery,"share-edit-list");
editMeta.innerHTML=LI.htmlUnencode(viewMeta.innerHTML);
editSummary.value=LI.htmlUnencode(viewSummary.innerHTML);
editTitle.value=LI.htmlUnencode(viewTitle.innerHTML)
}function saveEdit(evt){if(evt){YEvent.preventDefault(evt)
}if(editTitle.value){var titleLong=editTitle.value;
var titleShort=editTitle.value.substring(0,70);
YDom.get(viewTitle).innerHTML=(titleLong.length>70)?titleShort+"... ":titleLong
}else{editTitle.value=viewTitle.innerHTML
}var descLong=editSummary.value;
var descShort=editSummary.value.substring(0,250);
YDom.get(viewSummary).innerHTML=(descLong.length>250)?descShort+"... ":descLong;
showView()
}function showView(evt){if(evt){YEvent.preventDefault(evt)
}gallery=YDom.get("share-image");
YDom.insertBefore(gallery,"share-content");
shareMode.className="share-mode is-view-mode"
}function showAttach(evt){YEvent.preventDefault(evt);
shareMode.className="share-mode is-attach-mode";
LI.hide("attach");
if(attachInput){attachInput.focus()
}if(isGroupInstance){setSubmitButtonRef(submitBottom)
}LI.show(settings);
WebTracking.trackUserAction(config.trackShowAttach)
}function removeObject(){shareMode.className="share-mode";
shareView.innerHTML="";
LI.show("attach");
editSummary.value="";
editTitle.value="";
entityID.value="";
imageID.value=""
}function enableSubmit(){YDom.removeClass(submit,"disabled");
submit.disabled=false
}function disableSubmit(){YDom.addClass(submit,"disabled");
submit.disabled=true
}function hideCheckTextareaMessage(){var checkTextAreaCountDown=Y$("p.check-textarea-message",form,true);
if(checkTextAreaCountDown){checkTextAreaCountDown.parentNode.removeChild(checkTextAreaCountDown)
}}function typeIsXML(o){var document=(YAHOO.env.ua.ie)?o.responseXML.documentElement:o.responseXML;
return document
}function resetHeight(){var baseHeight=el.getAttribute("data-base-height")||"15";
if(baseHeight){LI.grow(el,baseHeight)
}}};
