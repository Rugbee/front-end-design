}else{LI.show("share-include-photo-wrapper")
}editSummary.value=LI.htmlUnencode(viewSummary.innerHTML);
editTitle.value=LI.htmlUnencode(viewTitle.innerHTML);
attachInput.value="";
WebTracking.trackUserAction(config.trackGetLinkSuccess)
}function getLinkObjectError(){shareMode.className="share-mode";
WebTracking.trackUserAction(config.trackGetLinkFail)
}function postStatus(evt){YEvent.preventDefault(evt);
shareAjax.value="true";
LI.clearFormErrors(form);
LI.GhostLabel.Manager.hide(form.id);
window.clearTimeout(pollTimeout);
YAHOO.util.Connect.abort(ajaxRequest,{},false);
